<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.kuge.mapper.AlbumServiceMapper">
    <select id="selectAlbum" resultType="com.example.kuge.pojo.Album">
        select *
        from album
        where album_name=#{album_name}
        and user_id=#{id}
    </select>
</mapper>